{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  // sqlite | [postgresql,pgsql] | [sqlserver,mssql] | mysql
  "DBType": "sqlite",
  // 启动时运行迁移
  "RunMigrationsAtStartup": false,
  "ConnectionStrings": {
    "DefaultConnection": "data source=/data/token.db",
    "LoggerConnection": "data source=/data/logger.db"
  },
  "RabbitMQ": {
    "ConnectionString": "",
    "Serializer": "MessagePack"
    // amqp://token:dd666666@localhost:5672
  },
  "Chat": {
    "Master": "",
    "Shared": {
      "EnableShareAd": true,
      "ShareCredit": 5000,
      "ShareLimit": 5
    },
    "FreeModel": {
      "EnableFree": false,
      "Items": [
        {
          "Model": [
            "gpt-3.5-turbo-0125"
          ],
          "Limit": 2
        }
      ]
    },
    "ModelMapping": {
      "Enable": false,
      "Models": {
        "gpt-4o": [
          {
            "Model": "gpt-4o",
            "Order": 2
          },
          {
            "Model": "gpt4o"
          }
        ]
      }
    }
  },
  "Cache": {
    "Type": "Memory",
    "ConnectionString": ""
  },
  "Jwt": {
    "Secret": "asd?fgahjwter.223(%^klqwter.123(%^werqwter.123(%^$%#",
    "EffectiveHours": 120
  },
  "Github": {
    "Endpoint": "https://github.com",
    "APIEndpoint": "https://api.github.com"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "./Logs/log-.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "AI Gateway"
    }
  }
}
